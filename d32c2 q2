#include <stdio.h>

int main() {
    long long num;
    int freq[10] = {0}; 

    printf("Enter an integer number: ");
    scanf("%lld", &num);

    if (num < 0)
        num = -num;  

    while (num > 0) {
        int digit = num % 10;
        freq[digit]++;
        num /= 10;
    }

    int maxDigit = 0, maxCount = freq[0];
    for (int i = 1; i < 10; i++) {
        if (freq[i] > maxCount) {
            maxCount = freq[i];
            maxDigit = i;
        }
    }

    printf("Digit occurring the most times = %d\n", maxDigit);
    printf("Frequency = %d\n", maxCount);

    return 0;
}
