#include <stdio.h> 
int main() { 
    int a, b, c ;
    printf (" enter three numbers with space: ");
    scanf ( "%d %d %d", &a, &b, &c );
    if ( a == b && b== c)
     { printf ( "all three numbers are equal"  ); 
     }
    
    
     else if ( a >= b && b>= c)
     { printf(" %d is largest num among three numbers ", a ); 
     }
      else if ( b >= a && a>= c)
     { printf(" %d is largest num among three numbers ", b ); 
     }
     else { printf( "%d is largest num among three numbers ", c );}
     
     return 0;
     }
