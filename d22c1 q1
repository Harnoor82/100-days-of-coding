#include <stdio.h>

int main() {
    int num, originalNum, digit, sum = 0, fact, i;

    printf("Enter a number: ");
    scanf("%d", &num);

    originalNum = num;  

    while(num > 0) {
        digit = num % 10;   
        fact = 1;

        for(i = 1; i <= digit; i++) {
            fact *= i;
        }

        sum += fact;   
        num /= 10;     
    }

    if(sum == originalNum)
        printf("%d is a Strong Number.", originalNum);
    else
        printf("%d is not a Strong Number.", originalNum);

    return 0;
}
